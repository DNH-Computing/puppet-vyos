    prefix-list <%= prefix_list %> {
<% if @description -%>
	description "<%= description %>"
<% end -%>
<% rules.each_pair do |key, value_hash| -%>
        rule <%= value_hash['rule'] %> {
            <%- if value_hash['action'] -%>
            action <%= value_hash['action'] %>
            <%- end -%>
            <%- if value_hash['description'] -%>
            description "<%= value_hash['description'] %>"
            <%- end -%>
            <%- if value_hash['le'] -%>
            le <%= value_hash['le'] %>
            <%- end -%>
            <%- if value_hash['ge'] -%>
            ge <%= value_hash['ge'] %>
            <%- end -%>
            <%- if value_hash['prefix'] -%>
            prefix <%= value_hash['prefix'] %>
            <%- end -%>
        }
<% end -%>
    }
